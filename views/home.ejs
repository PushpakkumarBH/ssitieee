<!----------- Scroll Reveal JS ------------->
<script>
  const sr = ScrollReveal({
    origin: 'top',
    distance: '80px',
    duration: 2000,
    reset: 'true'
  })

  // home section
  sr.reveal('.h-l-text', {})
  sr.reveal('.h-l-text i', { origin: 'right', opacity: 0, delay: 300 })
  sr.reveal('.h-r-text', { delay: 600 })
  sr.reveal('.profile', { origin: 'left', delay: 900, opacity: 0 })

  // skills section
  sr.reveal('.s-l-text', {})
  sr.reveal('.s-bars', {delay: 300})

  // hobbies section
  sr.reveal('.h-card:nth-child(1)', { origin: 'bottom', delay: 600 })
  sr.reveal('.h-card:nth-child(2)', { origin: 'bottom', delay: 800 })
  sr.reveal('.h-card:nth-child(3)', { origin: 'bottom', delay: 1000 })
 
  // language section
  sr.reveal('.l-icons .row-1 i:nth-child(1)', { origin: 'bottom' })
  sr.reveal('.l-icons .row-1 i:nth-child(2)', { origin: 'bottom', delay: 300 })
  sr.reveal('.l-icons .row-1 i:nth-child(3)', { origin: 'bottom', delay: 600 })
  sr.reveal('.l-icons .row-1 i:nth-child(4)', { origin: 'bottom', delay: 900 })
  
  sr.reveal('.l-icons .row-2 i:nth-child(1)', { origin: 'bottom', delay: 1200 })
  sr.reveal('.l-icons .row-2 i:nth-child(2)', { origin: 'bottom', delay: 1500 })
  sr.reveal('.l-icons .row-2 i:nth-child(3)', { origin: 'bottom', delay: 1800 })
  sr.reveal('.l-icons .row-2 i:nth-child(4)', { origin: 'bottom', delay: 2100 })
</script>
