<!-- CSS only -->

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
<div class="container">
<form class="form" action="" method="post">
    <h1>Register</h1>
    <div class="mb-3">
        <label for="name" class="form-label">Full Name</label>
        <input type="name" name="name" class="form-control" placeholder="Name" required>
    </div>
    <div class="mb-3">
        <label for="Institutename" class="form-label">Name of your institution</label>
        <input type="name" name="institutename" class="form-control" placeholder="Institute Name" required>
    </div>
    <div class="mb-3">
        <label for="Contactnum" class="form-label">Contact Number</label>
        <input type="number" name="contactnum" class="form-control" placeholder="Contact Num" required>
    </div>
    <div class="mb-3">
        <label for="email" class="form-label">Email address</label>
        <input type="email" name="email" class="form-control" placeholder="Email" required>
    </div>
    <div class="mb-3">
        <label for="Choose Event" class="form-label">Choose Event</label>
    <select class="form-select" aria-label="Default select example">
        <option selected name="event">Select</option>
        <option value="1">ISRO IEEE Talk</option>
        <option value="2">Space On Wheels</option>
        <option value="3">Both Events</option>
      </select>
    </div>
      <button class="btn btn-dark">Submit</button>
</form>
</div>
